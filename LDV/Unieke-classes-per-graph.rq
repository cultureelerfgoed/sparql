#+ name: Unieke-classes-per-graph
#+ endpoint: https://api.linkeddata.cultureelerfgoed.nl/datasets/rce/kunstcollecties-kc-2/services/kunstcollecties-kc-2/sparql

SELECT (REPLACE(STR(?g), "^urn:graph:", "") AS ?graphNaam)
       ?class
       (COUNT(*) AS ?gebruik)
WHERE {
  GRAPH ?g {
    ?s a ?class .
  }
}
GROUP BY ?g ?class
ORDER BY ?g DESC(?gebruik)
