#+ name: gezichtennummer-van-een-cho-1
#+ description: Query om bij een CHO objectnummer het gezichtennummer te vinden. Maar ook om bij een gezicht een CHO objectnummer te zoeken.
#+ endpoint: https://api.linkeddata.cultureelerfgoed.nl/datasets/rce/cho/sparql

prefix ceo: <https://linkeddata.cultureelerfgoed.nl/def/ceo#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix graph: <https://linkeddata.cultureelerfgoed.nl/graph/>
select * where {
  graph graph:instanties-rce {
    ?cho a ceo:Gezicht;
                   ceo:cultuurhistorischObjectnummer ?cultuurhistorischObjectNummer ;
                   ceo:gezichtsnummer ?gezichtsnummer .
  }
} limit 10