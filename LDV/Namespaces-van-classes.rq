#+ name: Namespaces-van-classes
#+ endpoint: https://api.linkeddata.cultureelerfgoed.nl/datasets/rce/kunstcollecties-kc-2/services/kunstcollecties-kc-2/sparql

SELECT ?namespace (COUNT(*) AS ?gebruik)
WHERE {
  GRAPH ?g { ?s a ?class }
  BIND(REPLACE(STR(?class), "(.*/).*", "$1") AS ?namespace)
}
GROUP BY ?namespace
ORDER BY DESC(?gebruik)

