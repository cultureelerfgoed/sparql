#+ name: training-wo2-query
#+ endpoint: https://api.linkeddata.cultureelerfgoed.nl/datasets/rce/thesauri/services/thesauri/sparql

prefix skos: <http://www.w3.org/2004/02/skos/core#>
prefix text: <http://jena.apache.org/text#>

select * where
{
  ?uri text:query (?predicate ?query).
  filter not exists {
    ?uri skos:inScheme <https://data.niod.nl/WO2_Thesaurus/11183>
  }

  ?uri ?predicate ?label.
  ?uri a skos:Concept.

  values ?predicate {
  skos:prefLabel skos:altLabel
}
}
limit 5