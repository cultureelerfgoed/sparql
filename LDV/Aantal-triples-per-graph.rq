#+ name: Aantal-triples-per-graph
#+ endpoint: https://api.linkeddata.cultureelerfgoed.nl/datasets/rce/kunstcollecties-kc-2/services/kunstcollecties-kc-2/sparql

SELECT (REPLACE(STR(?graph), "^urn:graph:", "") AS ?graphNaam)
       (COUNT(*) AS ?triples)
WHERE { 
  GRAPH ?graph { ?s ?p ?o } 
}
GROUP BY ?graph
ORDER BY DESC(?triples)
